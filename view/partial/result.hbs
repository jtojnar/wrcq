<table class="table table-striped table-bordered table-hover table-fixed-header table-condensed">
<thead class="header"><tr><th>Overall Place</th><th>Members</th><th>Category</th><th>Score</th><th>Time</th><th>Penalty</th><!-- weird category counters -->{{#if is.MO}}<th>MO</th>{{/if}}{{#if is.XO}}<th>XO</th>{{/if}}{{#if is.WO}}<th>WO</th>{{/if}}{{#if is.MV}}<th>MV</th>{{/if}}{{#if is.XV}}<th>XV</th>{{/if}}{{#if is.WV}}<th>WV</th>{{/if}}{{#if is.MSV}}<th>MSV</th>{{/if}}{{#if is.XSV}}<th>XSV</th>{{/if}}{{#if is.WSV}}<th>WSV</th>{{/if}}{{#if is.MUV}}<th>MUV</th>{{/if}}{{#if is.XUV}}<th>XUV</th>{{/if}}{{#if is.WUV}}<th>WUV</th>{{/if}}{{#if is.MJ}}<th>MJ</th>{{/if}}{{#if is.XJ}}<th>XJ</th>{{/if}}{{#if is.WJ}}<th>WJ</th>{{/if}}{{#if is.M20}}<th>M20</th>{{/if}}{{#if is.X20}}<th>X20</th>{{/if}}{{#if is.W20}}<th>W20</th>{{/if}}{{#if is.M23}}<th>M23</th>{{/if}}{{#if is.X23}}<th>X23</th>{{/if}}{{#if is.W23}}<th>W23</th>{{/if}}</tr></thead>
<tbody>
{{#teams}}
<tr><td>{{place}}</td><td>{{#each members}}{{lastname}} {{firstname}} ({{country_code}}){{#unless @last}}, {{/unless}}{{/each}}</td><td>{{category}}</td>{{#equals status 'finished'}}<td>{{score}}</td><td>{{time time}}</td><td>{{penalty}}</td>{{else}}<td colspan="3">{{status}}</td>{{/equals}}{{#if ../is.MO}}<td class="place{{MO}}">{{MO}}</td>{{/if}}{{#if ../is.XO}}<td class="place{{XO}}">{{XO}}</td>{{/if}}{{#if ../is.WO}}<td class="place{{WO}}">{{WO}}</td>{{/if}}{{#if ../is.MV}}<td class="place{{MV}}">{{MV}}</td>{{/if}}{{#if ../is.XV}}<td class="place{{XV}}">{{XV}}</td>{{/if}}{{#if ../is.WV}}<td class="place{{WV}}">{{WV}}</td>{{/if}}{{#if ../is.MSV}}<td class="place{{MSV}}">{{MSV}}</td>{{/if}}{{#if ../is.XSV}}<td class="place{{XSV}}">{{XSV}}</td>{{/if}}{{#if ../is.WSV}}<td class="place{{WSV}}">{{WSV}}</td>{{/if}}{{#if ../is.MUV}}<td class="place{{MUV}}">{{MUV}}</td>{{/if}}{{#if ../is.XUV}}<td class="place{{XUV}}">{{XUV}}</td>{{/if}}{{#if ../is.WUV}}<td class="place{{WUV}}">{{WUV}}</td>{{/if}}{{#if ../is.MJ}}<td class="place{{MJ}}">{{MJ}}</td>{{/if}}{{#if ../is.XJ}}<td class="place{{XJ}}">{{XJ}}</td>{{/if}}{{#if ../is.WJ}}<td class="place{{WJ}}">{{WJ}}</td>{{/if}}{{#if ../is.M20}}<td class="place{{M20}}">{{M20}}</td>{{/if}}{{#if ../is.X20}}<td class="place{{X20}}">{{X20}}</td>{{/if}}{{#if ../is.W20}}<td class="place{{W20}}">{{W20}}</td>{{/if}}{{#if ../is.M23}}<td class="place{{M23}}">{{M23}}</td>{{/if}}{{#if ../is.X23}}<td class="place{{X23}}">{{X23}}</td>{{/if}}{{#if ../is.W23}}<td class="place{{W23}}">{{W23}}</td>{{/if}}</tr>
{{/teams}}
</tbody>
</table>